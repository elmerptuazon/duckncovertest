<form action=<%= feedbacks_path(post) %> method="post">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token  %>">
    <p>User</p> <input type="text" name="feedback[user]" value="">
    <p>Content</p> <textarea name="feedback[content]" rows="6" cols="20"></textarea>
    <input type="submit" value="Submit">
</form>